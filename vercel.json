{
  "version": 2,
  "builds": [
    {
      "src": "packages/app-shell/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/packages/app-shell/$1"
    }
  ],
  "buildCommand": "cd packages/app-shell && yarn install --frozen-lockfile && yarn build",
  "outputDirectory": "packages/app-shell/.next"
}
